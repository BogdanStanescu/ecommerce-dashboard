datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Store {
  id         String   @id @default(uuid())
  name       String
  user_id    String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
